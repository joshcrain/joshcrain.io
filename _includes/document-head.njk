<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0"> 
<title>{{ metaTitle }}{% if metaTitle == blank %}{{ title }}{% endif %}{{ artworks.group }}{% if (title or artworks.group) == blank %}“{{ tag }}”{% endif %} - joshcrain.io</title>
<meta name="description" content="{{ description | safe }}">
<meta name="author" content="Josh Crain">
<meta name="generator" content="Eleventy">

{# Open Graph meta tags for social sharing #}
<meta property="og:title" content="{{ metaTitle }}{% if metaTitle == blank %}{{ title }}{% endif %}{{ artworks.group }}{% if (title or artworks.group) == blank %}"{{ tag }}"{% endif %} - joshcrain.io">
<meta property="og:description" content="{{ description | safe }}">
<meta property="og:type" content="{% if collections.notes %}article{% else %}website{% endif %}">
<meta property="og:url" content="https://joshcrain.io{{ page.url }}">
<meta property="og:site_name" content="joshcrain.io">
{% if page.inputPath.includes('/notes/') %}
<meta property="og:image" content="https://joshcrain.io{{ page.url }}index.jpg">
<meta property="og:image:width" content="1200">
<meta property="og:image:height" content="630">
<meta property="og:image:alt" content="Social image for {{ title }}">
{% endif %}

{# Twitter Card meta tags #}
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:site" content="@joshcrain">
<meta name="twitter:creator" content="@joshcrain">
{% if page.inputPath.includes('/notes/') %}
<meta name="twitter:image" content="https://joshcrain.io{{ page.url }}index.jpg">
<meta name="twitter:image:alt" content="Social image for {{ title }}">
{% endif %}

<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
<link rel="icon" href="favicon.ico" type="image/x-icon">
<link rel="manifest" href="/site.webmanifest">
<meta name="theme-color" content="rgb(239, 233, 228)">
<link rel="preload" href="/fonts/DMSerifDisplay-Regular.woff2" as="font" type="font/woff2" crossorigin>
{% set css %}{% include "styles.css" %}{% endset %}<style>{{ css | cssmin | safe }}</style>
<link rel="alternate" type="application/atom+xml" title="RSS Feed for joshcrain.io" href="/feed.xml" />
